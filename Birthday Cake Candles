public static int birthdayCakeCandles(List<Integer> candles) {
    // Write your code here
        int tallest = 0;
        int finalNum = 0;
        for(int i=0;i<candles.size();i++){
            if(candles.get(i)>tallest) tallest = candles.get(i);
        }
        for(int i=0;i<candles.size();i++){
            if(candles.get(i) == tallest) finalNum++;
        }
        return finalNum;
    }
